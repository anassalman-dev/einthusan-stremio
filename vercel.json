{
  "version": 2,
  "builds": [
    {
      "src": "api/[transport].js",
      "use": "@vercel/node"
    },
    {
      "src": "configure/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/configure",
      "dest": "/configure/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/[transport].js"
    }
  ]
}
